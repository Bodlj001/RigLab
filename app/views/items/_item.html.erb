<div id="<%= dom_id item %>">
  <p>
    <strong>Name:</strong>
    <%= item.name %>
  </p>

  <p>
    <strong>Type:</strong>
    <%= item.type %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= item.price %>
  </p>

  <p>
    <strong>Visual condition:</strong>
    <%= item.visual_condition %>
  </p>

  <p>
    <strong>Graphics:</strong>
    <%= item.graphics %>
  </p>

  <p>
    <strong>Storage type:</strong>
    <%= item.storage_type %>
  </p>

  <p>
    <strong>Storage quantity:</strong>
    <%= item.storage_quantity %>
  </p>

  <p>
    <strong>Processor:</strong>
    <%= item.processor %>
  </p>

  <p>
    <strong>Model:</strong>
    <%= item.model %>
  </p>

  <p>
    <strong>Model identifier:</strong>
    <%= item.model_identifier %>
  </p>

  <p>
    <strong>Serial number:</strong>
    <%= item.serial_number %>
  </p>

</div>
